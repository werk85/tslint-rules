{
  "compilerOptions": {
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "declaration": true,
    "sourceMap": false,
    "target": "es5",
    "outDir": "./lib"
  },
  "include": [
    "src/**/*"
  ]
}
